{
  "name": "codify-dev",
  "description": "Inspect panel on Figma, and ai tools.",
  "private": true,
  "version": "0.0.5",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build && pnpm run build:plugins && pnpm run build:readme",
    "build:firefox": "wxt build -b firefox",
    "build:plugins": "tsc -p ./plugins/tsconfig.json",
    "build:readme": "tsx ./build/readme.ts",
    "npm:plugins": "pnpm build:plugins && cd plugins && pnpm publish --access public && cd ..",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "vue-tsc --noEmit",
    "postinstall": "wxt prepare",
    "lint": "eslint src --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src *.cjs *.json *.md"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.100.2",
    "@rushstack/eslint-patch": "^1.10.4",
    "@types/prismjs": "^1.26.5",
    "@types/stringify-object": "^4.0.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vueuse/core": "^10.11.1",
    "@wxt-dev/module-vue": "^1.0.1",
    "comment-mark": "^1.1.1",
    "eslint": "^8.57.1",
    "eslint-plugin-perfectionist": "^3.9.1",
    "eslint-plugin-vue": "^9.30.0",
    "overlayscrollbars": "^2.10.0",
    "p-wait-for": "^5.0.2",
    "playwright-chromium": "^1.52.0",
    "prettier": "^3.3.3",
    "stringify-object": "^5.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vue": "^3.5.12",
    "vue-tsc": "^2.1.10",
    "wxt": "^0.19.13"
  },
  "pnpm": {
    "patchedDependencies": {
      "wxt@0.19.13": "patches/wxt@0.19.13.patch"
    }
  },
  "dependencies": {
    "@vue/repl": "^4.5.1",
    "jszip": "^3.10.1",
    "openai": "^4.90.0"
  },
  "onlyBuiltDependencies": [
    "playwright-chromium"
  ]
}
